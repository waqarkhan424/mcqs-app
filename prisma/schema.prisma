
generator client {
  provider = "prisma-client-js"
  output   = "./prisma/generated"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}



model Question {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  question      String
  options       String[]
  correctAnswer String
  solution      String?
  category      String?
}